{
  "name": "hook0-keycloak-theme",
  "version": "1.0.0",
  "keywords": [
    "keycloak",
    "theme",
    "keycloak-theme"
  ],
  "description": "Hook0 Theme for Keycloak",
  "scripts": {
    "preinstall": "cp -v ../mediakit/logo/512x512-white.png ./src/main/resources/theme/hook0-redesign/email/img/logo.png && cp -v ../mediakit/logo/512x512-white.png ./src/main/resources/theme/hook0-redesign/login/resources/img/logo.png && cp -v ../mediakit/logo/favicon.ico  ./src/main/resources/theme/hook0-redesign/login/resources/img/favicon.ico",
    "build": "parcel build --dist-dir=./src/main/resources/theme/hook0-redesign/login/resources/dist/ src/main/resources/theme/hook0-redesign/login/resources/scss/*.scss"
  },
  "author": "Francois-Guillaume Ribreau <npm@fgribreau.com> (https://fgribreau.com/)",
  "license": "MIT",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@parcel/transformer-image": "^2.8.0",
    "@parcel/transformer-sass": "^2.8.0",
    "@tailwindcss/forms": "^0.3.3",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "parcel": "^2.8.0",
    "postcss-fontpath": "^1.0.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.7"
  }
}
